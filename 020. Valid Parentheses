class Solution(object):
    def isValid(self, s):
        """
        :type s: str
        :rtype: bool
        """
        openBrackets = {
            '(': ')',
            '{': '}',
            '[': ']'
        }
        bracketsCount = {
            '(': 0,
            '{': 0,
            '[': 0
        }
        values = openBrackets.values()
        keys = openBrackets.keys()
        
        for i in range(len(s)):
          if s[i] in openBrackets:
            bracketsCount[s[i]] += 1
          if s[i] in openBrackets.values():
            key = [k for k, v in openBrackets.items() if v == s[i]]
            bracketsCount[key[0]] -= 1
        for b in bracketsCount:
          if bracketsCount[b] > 0:
            return False
        return True
